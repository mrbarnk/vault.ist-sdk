{
  "name": "vault.ist-sdk",
  "version": "1.2.7",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup ./src --format cjs,esm --dts",
    "dev": "nodemon src/index.ts",
    "test": "jest --config jestconfig.json",
    "prepublishOnlys": "npm run build && npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "prebuildpackage": "npm run clean && npm run build && node dist/index.js",
    "buildpackage": "cd dist && cp ../package.json ./ && npm pack && npm publish"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.12.7",
    "@types/sinon": "^17.0.3",
    "jest": "^29.5.0",
    "nodemon": "^3.1.0",
    "prettier": "^2.8.8",
    "sinon": "^17.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dotenv": "^16.4.5"
  }
}
